{
  "multipart":[
    {
      "apply":{
        "model":"thaumaturge:block/vessel"
      }
    },
    {
      "when":{
        "water_level":1
      },
      "apply":{
        "model":"thaumaturge:block/water_vessel_level1"
      }
    },
    {
      "when":{
        "water_level":2
      },
      "apply":{
        "model":"thaumaturge:block/water_vessel_level2"
      }
    },
    {
      "when":{
        "water_level":3
      },
      "apply":{
        "model":"thaumaturge:block/water_vessel_full"
      }
    },
    {
      "when":{
        "boiling":true
      },
      "apply":{
        "model":"thaumaturge:block/boiling_overlay"
      }
    },
    {
      "when":{
        "waterlogged":true
      },
      "apply":{
        "model":"thaumaturge:block/waterlogged_overlay"
      }
    }
  ]
}